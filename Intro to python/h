first_num = int(input("Enter a integer: "))
print("You entered:", first_num)

print(first_num, "Squared is:", first_num ** 2)

print(first_num "Cubed is:", first_num ** 3)

second_num = int(input("Enter a second number: "))
print(first_num "+" second_num "=" first_num + second_num)
print(first_num "*" second_num, "=" first_num * second_num)
